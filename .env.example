# Copy this file to `.env.local` for local development.
# Defaults fall back to local SQLite; override for production.
DATABASE_URL="file:./prisma/dev.db"

# Required in production. Replace with a strong secret.
NEXTAUTH_SECRET="change-me"

# Optional: override where user accounts are stored.
# Defaults to src/data/users.json relative to the project root.
USER_STORE_PATH="src/data/users.json"
